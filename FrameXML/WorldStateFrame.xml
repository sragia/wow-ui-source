			<OnEnter>
				GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
				GameTooltip:AddLine(self.tooltip, 1.0, 1.0, 1.0, true);
				GameTooltip:Show();
			</OnEnter>
			<OnClick>
				if ( self.sortType ) then
					SortBattlefieldScoreData(self.sortType);
				end
				PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
			</OnClick>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
